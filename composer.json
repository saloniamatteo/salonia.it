{
    "name": "saloniamatteo/salonia.it",
    "description": "Matteo Salonia's website",
    "type": "project",
    "homepage": "https://salonia.it",
    "support": {
        "source": "https://github.com/saloniamatteo/salonia.it"
    },
    "require": {
        "php": "^8.1",
        "codeigniter4/framework": "^4.5.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Config\\": "app/Config/"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app/",
            "Config\\": "app/Config/",
            "Tests\\Support\\": "tests/_support"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
